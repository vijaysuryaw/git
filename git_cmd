# Git Common commands

git init
git status
git add .
git commit -m "new commit"
git push origin main

git pull origin main

echo "# git" >> README.md
git init
git add README.md
git commit -m "first commit"
git branch -M main
git remote add origin https://github.com/vijaysuryaw/git.git
git push -u origin main



